{
    "env": {
        "browser": false,
        "commonjs": true,
        "es2020": true,
        "mocha": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 11
    },
    "rules": {
        "max-len": [
			1,
			120,
			8
		],
		"comma-dangle": [
			"error",
			"never"
        ],
		"indent": ["warn", 2],
		"no-console": "error",
        "no-unused-vars": [
            "error",
            {
              "varsIgnorePattern": "should|expect"
            }
        ]
    }
}
